    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    NavigationPage.HasBackButton="False">
    <ContentPage.Content>
        <StackLayout >
            <Label
                BackgroundColor="#cccccc"
                FontSize="25"
                HorizontalOptions="FillAndExpand"
