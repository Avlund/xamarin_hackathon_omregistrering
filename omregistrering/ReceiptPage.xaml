                    VerticalOptions="Center" />
                <Label
                    x:Name="line_3"
                    FontSize="20"
                    HorizontalOptions="Center"
                    VerticalOptions="Center" />
                <Label
                    x:Name="line_4"
                    FontSize="20"
                    HorizontalOptions="Center"
                    VerticalOptions="Center" />
                <Label
                    x:Name="line_5"
                    FontSize="20"
                    HorizontalOptions="Center"
                    VerticalOptions="Center" />
                <Label
                    x:Name="line_6"
                    FontSize="20"
                    HorizontalOptions="Center"
                    VerticalOptions="Center" />
                <Label
                    x:Name="line_7"
                    FontSize="20"
                    HorizontalOptions="Center"
                    VerticalOptions="Center" />
            </StackLayout>
        </StackLayout>

    </ContentPage.Content>
</ContentPage>